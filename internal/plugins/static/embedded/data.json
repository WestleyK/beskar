{
    "routes": [
        {
            "pattern": "/(artifacts/static/[0-9A-Za-z-_/]{1,})/file/([^/]+)$",
            "methods": [
                "GET",
                "HEAD"
            ]
        },
        {
            "pattern": "/artifacts/static/api/v1/doc/(.*)",
            "body": false
        },
        {
            "pattern": "/artifacts/static/api/v1/(.*)",
            "body": true,
            "body_key": "repository"
        }
    ],
    "mediatype": {
        "file": "application/vnd.ciq.static.v1.file"
    }
}